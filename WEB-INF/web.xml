<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">


    <servlet>
        <servlet-name>CatalogPage</servlet-name>
        <servlet-class>CatalogPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartPage</servlet-name>
        <servlet-class>CartPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckoutPage</servlet-name>
        <servlet-class>CheckoutPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmationPage</servlet-name>
        <servlet-class>ConfirmationPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignInPage</servlet-name>
        <servlet-class>SignInPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomePage</servlet-name>
        <servlet-class>HomePage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderPage</servlet-name>
        <servlet-class>OrderPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutoCompleteServlet</servlet-name>
        <servlet-class>AutoCompleteServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CatalogPage</servlet-name>
        <url-pattern>/CatalogPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartPage</servlet-name>
        <url-pattern>/CartPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckoutPage</servlet-name>
        <url-pattern>/CheckoutPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmationPage</servlet-name>
        <url-pattern>/ConfirmationPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignInPage</servlet-name>
        <url-pattern>/SignInPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomePage</servlet-name>
        <url-pattern>/HomePage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderPage</servlet-name>
        <url-pattern>/OrderPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutoCompleteServlet</servlet-name>
        <url-pattern>/autocomplete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>
